[distutils]
index-servers =
  pypi
  pypitest

[pypi]
repository=https://pypi.python.org/pypi
username=<username>
password=<password>

[pypitest]
repository=https://testpypi.python.org/pypi
username=<username>
password=<password>


# To use:
#
# $ python setup.py register -r pypi
# $ python setup.py sdist upload -r pypi
